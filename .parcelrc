{
    "extends": "@parcel/config-default",
    "packagers": {
        "*.html": "@parcel/transformer-html",
        "*.css": "@parcel/transformer-css"
    },
    "transformers": {
        "*.html": ["parcel-transformer-copy", { "destDir": "dist" }],
        "*.css": ["parcel-transformer-copy", { "destDir": "dist" }]
    }
}